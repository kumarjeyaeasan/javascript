<%- include("partials/header"); -%>

<div class="comp">
  <h1>Compose</h1>
  <form action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input class="form-control" type="text" name="titleText" />
      <label for="composeText">Post</label>
      <textarea
        class="form-control"
        name="composeText"
        id=""
        cols="30"
        rows="5"
      ></textarea>
    </div>
    <button class="btn btn-primary" type="submit" name="submit">Publish</button>
  </form>
</div>

<%- include("partials/footer"); -%>
